{
  "name": "ofablog",
  "version": "0.1.0",
  "author": {
    "email": "ofadiman@gmail.com",
    "name": "Szymon Jaworski",
    "url": "https://github.com/Ofadiman"
  },
  "description": "Personal blog.",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint-find-rules-deprecated": "eslint-find-rules --deprecated --no-error .eslintrc.js",
    "eslint-find-rules-unused": "eslint-find-rules --unused --no-error .eslintrc.js",
    "gen": "ts-node codegen.ts",
    "start": "next start",
    "syncpack-format": "syncpack format"
  },
  "dependencies": {
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ofadiman/plop": "0.1.3",
    "@prettier/plugin-xml": "0.12.0",
    "@types/chalk": "2.2.0",
    "@types/node": "14.14.16",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "chalk": "4.1.0",
    "eslint": "7.16.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.4",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-testing-library": "3.10.1",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "eslint-plugin-unicorn": "24.0.0",
    "execa": "5.0.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "syncpack": "5.6.10",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "syncpack": {
    "dev": true,
    "peer": true,
    "prod": true,
    "sortFirst": [
      "name",
      "version",
      "author",
      "description",
      "main",
      "typings",
      "license",
      "scripts",
      "dependencies",
      "devDependencies",
      "peerDependencies",
      "eslintConfig",
      "commitlint",
      "husky",
      "lint-staged",
      "prettier",
      "syncpack"
    ],
    "versionGroups": []
  },
  "bugs": "https://github.com/Ofadiman/ofablog/issues",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "articles",
    "blog",
    "tutorial"
  ],
  "private": true,
  "repository": "Ofadiman/ofablog"
}
