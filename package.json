{
  "name": "ofablog",
  "version": "0.1.0",
  "author": {
    "email": "ofadiman@gmail.com",
    "name": "Szymon Jaworski",
    "url": "https://github.com/Ofadiman"
  },
  "description": "Personal blog.",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint-check": "eslint ./**/*.{ts,tsx}",
    "eslint-fix": "eslint ./**/*.{ts,tsx} --fix",
    "npm-update-minor": "ncu -u --target minor",
    "start": "next start",
    "storybook-build": "build-storybook --output-dir storybook-dist",
    "storybook-dev": "start-storybook -p 6006",
    "syncpack-format": "syncpack format",
    "test": "jest --watch",
    "test-ci": "CI=true jest"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@react-aria/button": "^3.3.1",
    "@react-aria/interactions": "^3.3.3",
    "date-fns": "^2.16.1",
    "faker": "^5.5.2",
    "gray-matter": "^4.0.2",
    "next": "10.0.5",
    "next-mdx-remote": "^2.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-headroom": "^3.1.0",
    "react-icons": "^4.2.0",
    "react-responsive": "^8.2.0",
    "react-spring": "^9.0.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ofadiman/eslint-config-base": "^0.1.8",
    "@ofadiman/eslint-config-react": "^0.1.3",
    "@ofadiman/plop-utils": "^0.3.0",
    "@prettier/plugin-xml": "0.12.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/chalk": "2.2.0",
    "@types/faker": "^5.5.0",
    "@types/jest": "^26.0.22",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-headroom": "^2.2.1",
    "@types/react-responsive": "^8.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "chalk": "4.1.0",
    "eslint": "^7.18.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "execa": "5.0.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "lint-staged": "10.5.3",
    "npm-check-updates": "^10.2.5",
    "plop": "2.7.4",
    "prettier": "^2.2.1",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "syncpack": "5.6.10",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "improvement",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{svg,md,mdx}": [
      "prettier --write"
    ]
  },
  "syncpack": {
    "dev": true,
    "peer": true,
    "prod": true,
    "sortFirst": [
      "name",
      "version",
      "author",
      "description",
      "main",
      "typings",
      "license",
      "scripts",
      "dependencies",
      "devDependencies",
      "peerDependencies",
      "eslintConfig",
      "commitlint",
      "husky",
      "lint-staged",
      "prettier",
      "syncpack"
    ],
    "versionGroups": []
  },
  "bugs": "https://github.com/Ofadiman/ofablog/issues",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "articles",
    "blog",
    "tutorial"
  ],
  "private": true,
  "repository": "Ofadiman/ofablog"
}
